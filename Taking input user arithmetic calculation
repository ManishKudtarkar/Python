a = int (input("Enter number 1:-"))
b = int (input("Enter number 2:-"))
def add(a,b):
  return a+b
  
print("The sum is", add(a,b))

def multi(a,b):
  return a*b
  
print("The multiplication  is", multi(a,b))

def division(a,b):
  return a/b
  
print("The division is", division(a,b))

def sub(a,b):
  return a-b
  
print("The subtraction is", sub(a,b))
